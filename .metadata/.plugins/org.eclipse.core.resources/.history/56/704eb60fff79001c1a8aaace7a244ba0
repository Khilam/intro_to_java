package fr.mouna.introductiontojava;

//class principal
public class Main {

	public static void main(String[] args) {
		//affectation
		int age = 20;
		switch (age) {
		case 10:
			System.out.println("enfant");
		break;
		case 17:
			System.out.println("mineur");
		break;
		case 18:
			System.out.println("majeur");
		break;
		default: System.out.println("No information");
			
			
		}
		
	
	

	}

}

package fr.mouna.introductiontojava;

//class principal
public class Main {

	public static void main(String[] args) {
		//affectation
	int money = 1000;
	if (money != 2000 || money != 1500) {
		System.out.println("c'est vari !");
	}else {
		
		System.out.println ("ce n'est pas vari !");
	}
	

	}

}
